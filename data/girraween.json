            "contentItemCollection": {
                "items": [{
                    "type": "RICH_TEXT",
                    "text": "<p>Which statement explains why this programming pattern is common?</p>"
                }]
            }
        },
        "answer": {
            "parameters": {
                "type": "MULTI_CHOICE",
                "settings": { "multipleCorrectOptions": false },
                "options": [
                    { "text": "As the loop allows for repetitive system design." },
                    { "text": "So the hardware can be configured before the software implementation." },
                    { "text": "Because it is a requirement of the IDE." },
                    { "text": "As the loop needs to happen after setup." }
                ]
