      },
      "sampleAnswerContentItem": {
        "id": "34efb3e7-1460-4c5f-9de4-3fa13ac5edc4",
        "updatedDate": "2025-08-22T04:22:00.000Z",
        "files": null,
        "type": "RICH_TEXT",
        "text": "<p>SELECT ProductName, ProductPrice FROM Products<br>WHERE ProductPrice &lt;= 22.00</p>"
